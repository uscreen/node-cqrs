{
  "name": "@uscreen.de/cqrs-kit",
  "version": "0.34.12",
  "description": "CQRS Starter Kit to start fast into DDD. Eventsourcing included. Some soldering required.",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com:uscreen/node-cqrs.git"
  },
  "author": "Marcus Spiegel <spiegel@uscreen.de>",
  "contributors": [
    "@stas_natalenko"
  ],
  "files": [
    "src"
  ],
  "services": [
    "nats",
    "mongo",
    "redis"
  ],
  "license": "MIT",
  "keywords": [
    "cqrs",
    "ddd",
    "eventsourcing"
  ],
  "engines": {
    "node": ">=12.x"
  },
  "scripts": {
    "lint": "eslint '**/*.js' --fix",
    "test": "tap --node-arg=--no-warnings --no-check-coverage test/**/*.test.js",
    "test:cov": "tap --node-arg=--no-warnings --no-check-coverage --coverage-report=html test/**/*.test.js",
    "test:ci": "tap --node-arg=--no-warnings --no-check-coverage --coverage-report=text-summary test/**/*.test.js",
    "prepare": "husky install"
  },
  "dependencies": {
    "assert-plus": "^1.0.0",
    "cli-progress": "^3.10.0",
    "lock": "^1.1.0",
    "nats": "^1.4.12",
    "redis-lock": "^0.1.4",
    "rfdc": "^1.3.0",
    "uuid": "^8.3.1"
  },
  "devDependencies": {
    "@uscreen.de/dev-service": "0.11.2",
    "@uscreen.de/eslint-config-prettystandard-node": "^0.2.0",
    "env-schema": "^3.5.1",
    "husky": ">=6",
    "ioredis": "^4.28.2",
    "lint-staged": ">=10",
    "mongodb": "^3.7.3",
    "tap": "^15.1.6"
  },
  "lint-staged": {
    "*.js": "eslint --cache --fix"
  }
}
