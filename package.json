{
  "name": "@uscreen.de/cqrs-kit",
  "version": "0.30.2",
  "description": "CQRS Starter Kit to start fast into DDD. Eventsourcing included. Some soldering required.",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com:uscreen/node-cqrs.git"
  },
  "author": "Marcus Spiegel <spiegel@uscreen.de>",
  "contributors": [
    "@stas_natalenko"
  ],
  "license": "MIT",
  "keywords": [
    "cqrs",
    "ddd",
    "eventsourcing"
  ],
  "engines": {
    "node": ">=8.x"
  },
  "scripts": {
    "lint": "eslint '**/*.js' --fix",
    "test": "tap test/**/*.test.js",
    "test:cov": "tap --coverage-report=html test/**/*.test.js",
    "test:ci": "tap --100 --coverage-report=text-summary test/**/*.test.js"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js}": [
      "eslint --fix",
      "git add"
    ]
  },
  "dependencies": {
    "assert-plus": "^1.0.0",
    "lock": "^1.1.0",
    "nats": "^1.4.0",
    "redis-lock": "^0.1.4",
    "rfdc": "^1.1.4",
    "uuid": "^3.4.0"
  },
  "devDependencies": {
    "env-schema": "^1.1.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-config-standard": "^14.1.0",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-node": "^11.0.0",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "ioredis": "^4.14.1",
    "lint-staged": "^10.0.7",
    "mongodb": "^3.5.2",
    "nats-cli": "^1.1.0",
    "prettier": "^1.18.2",
    "tap": "^14.10.6",
    "yorkie": "^2.0.0"
  }
}
